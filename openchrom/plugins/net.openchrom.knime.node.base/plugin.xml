<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
<extension
         point="org.knime.workbench.repository.categories">
      <category
            level-id="openchrom"
            name="OpenChrom"
            icon="/icons/openchrom.png"
            description="OpenChrom nodes."
            path="/">
      </category>
       <category
            level-id="nmr"
            name="NMR"
            icon="/icons/nmr.png"
            description="OpenChrom NMR (Nuclear Magnetic Resonance Spectroscopy) nodes."
            path="/openchrom">
      </category>      
</extension> 
 <extension
         point="org.knime.workbench.repository.categories">
   <category
            level-id="util"
            name="Util"
            description="OpenChrom utility nodes."
            path="/openchrom">
    </category>
</extension>
<extension
         point="org.knime.core.PortType">
      <portType
            color="#A3000D"
            hidden="false"
            name="OpenChrom FID Measurement"
            objectClass="net.openchrom.knime.node.base.FIDPortObject"
            objectSerializer="net.openchrom.knime.node.base.FIDPortObject$Serializer"
            specClass="net.openchrom.knime.node.base.GenericPortObjectSpec"
            specSerializer="net.openchrom.knime.node.base.GenericPortObjectSpec$Serializer">
      </portType>
   </extension>
   <extension
         point="org.knime.core.PortType">
      <portType
            color="#337DFF"
            hidden="false"
            name="OpenChrom NMR Measurement"
            objectClass="net.openchrom.knime.node.base.NMRPortObject"
            objectSerializer="net.openchrom.knime.node.base.NMRPortObject$Serializer"
            specClass="net.openchrom.knime.node.base.GenericPortObjectSpec"
            specSerializer="net.openchrom.knime.node.base.GenericPortObjectSpec$Serializer">
      </portType>
   </extension>
</plugin>
