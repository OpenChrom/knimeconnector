<?xml version="1.0" encoding="UTF-8"?>
<plugin>
   <extension
         point="org.knime.workbench.repository.categories">
      <category
            level-id="nmr"
            name="NMR"
            icon="icons/nmr.png"
            description="Nuclear Magnetic Resonance Spectroscopy"
            path="/openchrom">
      </category>      
      <category
            level-id="io"
            name="I/O"
            icon="icons/io.png"
            description="Data input and output of various file formats."
            path="/openchrom/nmr">
      </category>
      <category
            level-id="processor"
            name="Processor"
            icon="icons/filter.png"
            description="Process data"
            path="/openchrom/nmr">
      </category>
      <category
            level-id="translator"
            name="Translator"
            icon="icons/translator.png"
            description="Translator"
            path="/openchrom/nmr">
      </category>		                     
   </extension>
   
   <extension
         point="org.knime.workbench.repository.nodes">
       <node
            category-path="/openchrom/nmr/translator"
            factory-class="net.openchrom.nmr.process.supplier.knime.ui.header2table.Header2TableNMRNodeFactory">
      </node>
      <node
            category-path="/openchrom/nmr/translator"
            factory-class="net.openchrom.nmr.process.supplier.knime.ui.tablefidtranslator.CsFidToTableNodeFactory">
      </node>
      <node
            category-path="/openchrom/nmr/translator"
            factory-class="net.openchrom.nmr.process.supplier.knime.ui.tablenmrtranslator.CsNmrToTableNodeFactory">
      </node>
       <node
            category-path="/openchrom/nmr/io"
            factory-class="net.openchrom.nmr.process.supplier.knime.ui.reader.ReaderNMRNodeFactory">
      </node>
      
      <node
            category-path="/openchrom/nmr/io"
            factory-class="net.openchrom.nmr.process.supplier.knime.ui.export.ScanExportNodeFactory">
      </node>
       <node
            category-path="/openchrom/nmr/io"
            factory-class="net.openchrom.nmr.process.supplier.knime.ui.listfiles.ListFilesReaderNMRNodeFactory">
      </node>
      <node
            category-path="/openchrom/nmr/io"
            factory-class="net.openchrom.nmr.process.supplier.knime.ui.listfilesfolder.ListFilesFolderReaderNMRNodeFactory">
      </node>
   </extension>
   <extension
         point="org.knime.workbench.repository.nodesets">
      <nodeset
            default-category-icon="icons/filter.png"
            deprecated="false"
            factory-class="net.openchrom.nmr.process.supplier.knime.ui.processor.ScanProcessorNodeSetFactory">
      </nodeset>
   </extension>
</plugin>
