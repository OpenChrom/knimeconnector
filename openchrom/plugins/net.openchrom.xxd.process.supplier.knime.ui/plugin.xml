<?xml version="1.0" encoding="UTF-8"?>
<plugin>
   <extension
         id="net.openchrom.xxd.process.supplier.knime.ui.fragment"
         point="org.eclipse.e4.workbench.model">
      <fragment
            uri="fragment.e4xmi">
      </fragment>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            name="KNIME Integration"
            category="org.eclipse.chemclipse.chromatogram.msd.process.ui.preferences.processPreferencePage"
            class="net.openchrom.xxd.process.supplier.knime.ui.preferences.PreferencePage"
            id="net.openchrom.chromatogram.xxd.process.supplier.knime.ui.preferences.preferencePage">
      </page>
   </extension>
   <extension
         point="org.knime.workbench.repository.categories">
      <category
            level-id="openchrom"
            name="OpenChrom"
            icon="icons/openchrom.png"
            description="OpenChrom I/O and visualization nodes."
            path="/">
      </category>
      <category
            level-id="msd"
            name="MSD"
            icon="icons/msd.png"
            description="Mass Spectrum Detector"
            path="/openchrom">
      </category>      
      <category
            level-id="io"
            name="I/O"
            icon="icons/io.png"
            description="Data input and output of various file formats."
            path="/openchrom/msd">
      </category>
      <category
            level-id="da"
            name="Direct Access"
            icon="icons/da.png"
            description="Direct access to measurement data via open editor."
            path="/openchrom/msd">
      </category>
      <category
            level-id="visualization"
            name="Visualization"
            icon="icons/visualization.png"
            description="Data visualization"
            path="/openchrom">
      </category>
      <category
            level-id="filter"
            name="Filter"
            icon="icons/filter.png"
            description="Filter"
            path="/openchrom/msd">
      </category>  
      <category
            level-id="translator"
            name="Translator"
            icon="icons/translator.png"
            description="Translator"
            path="/openchrom/msd">
      </category>  
      <category
      		level-id="process_control"
      		name="Process Control"
      		description="Process Control"
      		path="/openchrom/msd">
      </category>
      <category
            level-id="nmr"
            name="NMR"
            icon="icons/msd.png"
            description="Nuclear Magnetic Resonance Spectroscopy"
            path="/openchrom">
      </category>      
      <category
            level-id="io"
            name="I/O"
            icon="icons/io.png"
            description="Data input and output of various file formats."
            path="/openchrom/nmr">
      </category>         		                     
   </extension>
   <extension
         point="org.knime.workbench.repository.nodes">
      <node
            category-path="/openchrom/msd/io"
            factory-class="net.openchrom.xxd.process.supplier.knime.ui.io.msd.ChromatogramReaderMSDNodeFactory">
      </node>
      <node
            category-path="/openchrom/msd/io"
            factory-class="net.openchrom.xxd.process.supplier.knime.ui.io.msd.ChromatogramWriterMSDNodeFactory">
      </node>
      <node
            category-path="/openchrom/msd/da"
            factory-class="net.openchrom.xxd.process.supplier.knime.ui.da.msd.ChromatogramGetterMSDNodeFactory">
      </node>
      <node
            category-path="/openchrom/msd/da"
            factory-class="net.openchrom.xxd.process.supplier.knime.ui.da.msd.ChromatogramSetterMSDNodeFactory">
      </node>
      <node
            category-path="/openchrom/visualization"
            factory-class="net.openchrom.xxd.process.supplier.knime.ui.viz.chromatogram.ChromatogramNodeFactory">
      </node>
      <node
            category-path="/openchrom/visualization"
            factory-class="net.openchrom.xxd.process.supplier.knime.ui.viz.massspectrum.MassSpectrumNodeFactory">
      </node>
      <node
            category-path="/openchrom/msd/translator"
            factory-class="net.openchrom.xxd.process.supplier.knime.ui.translator.msd.CsMsdToTableNodeFactory">
      </node>
      <node
            category-path="/openchrom/msd/translator"
            factory-class="net.openchrom.xxd.process.supplier.knime.ui.translator.msd.TableToCsMsdNodeFactory">
      </node>
      <node
      		category-path="/openchrom/msd/process_control"
      		factory-class="net.openchrom.xxd.process.supplier.knime.ui.processing.msd.ProcessControllerMSDStartNodeFactory">
      </node>    
      <node
      		category-path="/openchrom/msd/process_control"
      		factory-class="net.openchrom.xxd.process.supplier.knime.ui.processing.msd.ProcessControllerMSDEndNodeFactory">
      </node>
      <node
            category-path="/openchrom/nmr/io"
            factory-class="net.openchrom.xxd.process.supplier.knime.ui.io.nmr.MeasurementReaderNMRNodeFactory">
      </node>                
   </extension>
</plugin>
