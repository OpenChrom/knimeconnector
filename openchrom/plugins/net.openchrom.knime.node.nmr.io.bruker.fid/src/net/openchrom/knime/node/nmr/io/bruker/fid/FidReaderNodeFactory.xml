<knimeNode icon="/icons/io.png" type="Source"
	xmlns="http://knime.org/node/v2.8"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://knime.org/node/v2.10 http://knime.org/node/v2.10.xsd">
	<name>Bruker FID Reader</name>

	<shortDescription>
		A node to read-in Bruker's FID data.
	</shortDescription>

	<fullDescription>

		<intro>Reads-in one Bruker FID file and transforms it into an OpenChrom FID
			data set.</intro>
			
		<option name="File Chooser">A Directory containing the <pre>fid</pre> file.
		</option> 
	</fullDescription>

	<ports>

		<outPort index="0" name="OpenChrom FID data">OpenChrom FID data.</outPort>

	</ports>



</knimeNode>
